[Unit]
Description=Symfony messenger products-stocks %i
StartLimitBurst=5
StartLimitIntervalSec=0

[Service]
ExecStart=php /.....path..../bin/console messenger:consume products-stocks --memory-limit=128m --time-limit=3600 --limit=100
Restart=always

User=unit
Group=unit

[Install]
WantedBy=default.target
